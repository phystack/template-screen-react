{
  "name": "phystack-react-app",
  "version": "0.4.0",
  "private": true,
  "homepage": "./",
  "type": "module",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "predev": "node scripts/init-settings.js",
    "dev": "yarn schema && vite",
    "start": "yarn dev",
    "schema": "mkdirp build && node scripts/build-schema.js && yarn analytics-schema",
    "analytics-schema": "node scripts/build-analytics-schema.js",
    "bundle": "phy app build ./build ./build",
    "build": "tsc -b && vite build && yarn schema && node scripts/post-build.js && yarn bundle",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "download-settings": "phy app settings",
    "pub": "phy app publish",
    "upload-description": "phy app upload-description",
    "connect": "phy dev ws"
  },
  "dependencies": {
    "@ombori/grid-reports": "^3.37.4",
    "@phystack/hub-client": "^4.4.31",
    "events": "^3.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@phystack/cli": "^4.4.12",
    "@phystack/ts-schema": "^4.4.12",
    "@types/node": "^24.6.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.25",
    "@vitejs/plugin-react": "^5.0.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "json-schema-defaults": "^0.4.0",
    "mkdirp": "^1.0.4",
    "ncp": "^2.0.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}
